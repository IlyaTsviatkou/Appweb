<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<table class="table">
    <tr><th></th><th>Email</th><th>Name</th><th>DateReg</th><th>Datelog</th><th></th></tr>
    @{int i = 0;}
    @foreach (var user in Model)
    {

        <tr>
            <td><input type="checkbox" name="@i" id="ch@i" onchange="OnChange"></td>

            <td>@user.Email</td>
            <td>@user.Name</td>
            
            <td>@user.DateReg</td>
            <td>@user.DateLog</td>
            <td>
                <form asp-action="Delete" asp-route-id="@user.Id" method="post">
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@user.Id">Change</a>
                    <button type="submit" class="btn btn-sm btn-danger">
                        Delete

                    </button>
                </form>
            </td>
        </tr>
    }
</table>